<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="board">
	<insert id="addBoard1" parameterType="java.util.HashMap">
 		insert into board values(board_seq.nextval, #{bigcate}, #{smallcate},#{title}, null, #{price}, #{writer}, sysdate, #{content})	
	</insert>
	<insert id="addBoard2" parameterType="java.util.HashMap">
	 	insert into board values(board_seq.nextval, #{bigcate}, #{smallcate},#{title}, #{imgpath}, #{price}, #{writer}, sysdate, #{content})
	</insert>
</mapper>